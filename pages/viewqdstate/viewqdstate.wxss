/* pages/viewqdstate/viewqdstate.wxss */
.logo{
  width: 100%;
}
.csselogo{
  margin-top:10px;
  max-height:100rpx;
  margin-bottom: 10px;
}
.table{
  font-size:15px;
  border: 1px solid #ccc;
  width: 100%;
}
.tr{
  width: 100%;
  display: flex;
  justify-content: space-between;
}
.th{
  text-align: center;
  font-weight: 900;
  color:#10aeff;
}
.thindex{
  width:20%;
}
.thname{
  width:50%;
}
.thtype{
  width:30%;
}
.th,.td{
  /* padding: 10px; */
  width: 100%;
  text-align: center;
}
.td{
  /* width: 100%; */
  font-size: 14px; 
  height: auto;
  line-height: auto;
  border-top: 1px solid #ccc; 
  border-left: 1px solid #ccc;
}
button{
  width: 60px;
  height: 30px;
  font-size: 14px;
  text-align: center;
  line-height: 30px;
  margin-top: 2px; 
}
.mylist{
  margin: 2px 10px;
  font-size: 15px;
  line-height: 15px;
  color: #10aeff;
}
.myline{
  height: 15px;
  line-height: 15px;
}
.noticetitle{
  margin-bottom: 10px;
  text-align: center;
  font-weight: bold;
}